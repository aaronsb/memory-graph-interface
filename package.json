{
  "name": "memory-graph-interface",
  "version": "1.0.0",
  "description": "A visualization tool for memory graph database",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "docker build -t memory-graph-interface:latest .",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "build:js": "webpack",
    "build:js:prod": "NODE_ENV=production webpack",
    "extract-modules": "node scripts/extract-modules.js",
    "watch": "webpack --watch",
    "dev": "concurrently \"npm run start\" \"npm run watch\""
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
